{
  "General": {
    "Service": {
      "Name": "localhost-icompass-daqdatastreamworker",
      "IsDebug": false, // true | false
      "Url": "http://localhost:7288",
      "Environment": "Test"
    },
    "Logging": {
      "LoggerType": "SerilogDebug", // ApplicationInsights | Console | Null | SerilogApplicationInsights | SerilogConsole | SerilogDebug | SerilogDatadog
      "LogWebRequests": "false",
      "LogWebRequestBody": "false",
      "LogWebRequestResponseBody": "false",
      "Debug": {
        "Enabled": "true",
        "LogLevel": "Information"
      }
    }
  },
  "ShmDatabase": {
    "ConnectionString": "Server=J-Desktop;Initial Catalog=icompass_shm_2;MultipleActiveResultSets=True;Connection Timeout=30;Integrated Security=True;TrustServerCertificate=True;"
  },
  "EventLogger": {
    "ConnectionString": "localhost:6379",
    "SensorChannelName": "sensor",
    "ShmChannelName": "shm"
  },
  "WorkerIteration": {
    "DefaultTake": 1000,
    "MaxCycleCountInEachLoop": 10,
    "MetaRefreshIntervalInSeconds": 1200,
    "DataParseIntervalInSeconds": 30,
    "MaxSensorsToParse": 1 
  },
  "SensorDatabase": {
    "ConnectionString": "Server=J-Desktop;Initial Catalog=iCompassSensor;MultipleActiveResultSets=True;Connection Timeout=30;Integrated Security=True;TrustServerCertificate=True;",
    "TimestampColumnName": "TmStamp",
    "ColumnMetaTableMappings": {
      "BaseTableName": "LNDBColumnMeta",
      "TableId": "LNDBTableMeta_tableID",
      "ActiveColumnName": "Active",
      "DbColumnName": "dbColumnName",
      "ColumnId": "columnID",
      "StationId": "LNDBStationMeta_stationID"
    },
    "TableMetaTableMappings": {
      "BaseTableName": "LNDBTableMeta",
      "TableId": "tableID",
      "DbTableName": "dbTableName"
    }
  }
}
